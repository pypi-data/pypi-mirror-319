<Engine xmlns="http://www.edkrule.org/XMLSchema"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.edkrule.org/XMLSchema">
    <DataTypes package="dp1">
        <DataType name="realNumber" class="dc1"/>
        <DataType name="VarIable" class="dc2" package="dp2"/>
    </DataTypes>
    <Identifiers package="sp1">
        <Identifier name="xx" class="sc1" />
        <Identifier name="yyy" class="sc2" package="sp2"/>
        <Identifier name="dasdsa" class="ic" package="sp"/>
    </Identifiers>
    <Expressions package="ep1">
        <Expression name="+" class="ec1" />
        <Expression name="-" class="ec2" package="ep2" />
        <Expression name="||" class="ec3" package="ep" />
        <Expression name="&amp;&amp;" class="ec4" package="ep" />
    </Expressions>
</Engine>