{
	"name": "@modelly/theme",
	"version": "0.3.0",
	"description": "Modelly UI packages",
	"type": "module",
	"main": "src/index.ts",
	"author": "",
	"license": "ISC",
	"private": false,
	"scripts": {
		"generate": "pollen -c src/pollen.config.cjs",
		"package": "svelte-package --input=. --cwd=../../.config/"
	},
	"exports": {
		".": {
			"modelly": "./src/index.ts",
			"import": "./dist/src/index.js",
			"types": "./dist/src/index.d.ts"
		},
		"./reset.css": {
			"modelly": "./src/reset.css",
			"import": "./dist/src/reset.css"
		},
		"./global.css": {
			"modelly": "./src/global.css",
			"import": "./dist/src/global.css"
		},
		"./tokens": {
			"modelly": "./src/tokens.ts",
			"import": "./dist/src/tokens.js"
		},
		"./typography.css": {
			"modelly": "./src/typography.css",
			"import": "./dist/src/typography.css"
		},
		"./pollen.css": {
			"modelly": "./src/pollen.css",
			"import": "./dist/src/pollen.css"
		},
		"./package.json": "./package.json"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"main_changeset": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/khulnasoft/modelly.git",
		"directory": "js/theme"
	}
}
